a:4:{s:5:"child";a:1:{s:0:"";a:1:{s:3:"rss";a:1:{i:0;a:6:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:1:{s:7:"version";s:3:"2.0";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:1:{s:0:"";a:1:{s:7:"channel";a:1:{i:0;a:6:{s:4:"data";s:0:"";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:1:{s:0:"";a:10:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:14:"Circle Citizen";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:48:"http://www.indianapolismonthly.com/circlecitizen";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:8:"language";a:1:{i:0;a:5:{s:4:"data";s:5:"en-US";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:9:"copyright";a:1:{i:0;a:5:{s:4:"data";s:35:"Copyright 2013, IndianapolisMonthly";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:13:"lastBuildDate";a:1:{i:0;a:5:{s:4:"data";s:29:"Tue, 13 Aug 2013 12:33:00 GMT";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:29:"Tue, 13 Aug 2013 12:33:00 GMT";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:9:"generator";a:1:{i:0;a:5:{s:4:"data";s:27:"http://emmisinteractive.com";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:5:"image";a:1:{i:0;a:6:{s:4:"data";s:0:"";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:1:{s:0:"";a:3:{s:3:"url";a:1:{i:0;a:5:{s:4:"data";s:832:"http://www.indianapolismonthly.com/EI/sharedobjects/handlers/ir.ashx?&w=144&mw=400";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:14:"Circle Citizen";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:48:"http://www.indianapolismonthly.com/circlecitizen";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}}}}}s:4:"item";a:30:{i:0;a:6:{s:4:"data";s:0:"";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:5:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:39:"Oldie but Goodie: Styx Rocks State Fair";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:1678:"<p><span class="title">My mom introduced me to Styx.</span> She used to pop in the greatest hits album when we were driving down the interstate and let the sweet rock &rsquo;n&lsquo; roll sing through the speakers. I didn&rsquo;t follow the band or know a lot about them, just that someone wanted to live with a woman named Lorelei, and that I could almost smell the sea as we sailed away together.</p>
<p>&nbsp;&ldquo;This is real music,&rdquo; Mom would tell me between fist pumps and head bobs.</p>
<p>Styx played the Marsh Free Stage at the <a title="State Fair photos" href="http://www.indianapolismonthly.com/PhotoPages/Photos.aspx?AlbumID=141469">Indiana State Fair</a> last night, and naturally Mom and I were there, surrounded by several generations of fans. The 40-year-old band took the stage wearing the kind of long hair and tight pants that were big in the &rsquo;70s. The audience cheered, clapped, fist-pumped, and belted out the words to old classics like &ldquo;Blue Collar Man&rdquo; and &ldquo;The Grand Illusion.&rdquo;</p>
<p>&ldquo;A lot of you weren&rsquo;t around when we first started,&rdquo; guitarist Tommy Shaw said to the crowd. Some clearly were: One man held a sign that said he had been to 40 Styx concerts. Chuck Panozzo, the original, 65-year-old bass player, made a guest appearance for &ldquo;Fooling Yourself.&rdquo;</p>
<p>These guys may be getting older, but they played for two hours&mdash;with an encore. And they sounded exactly like the greatest-hits album that blared through the speakers as we sped down the highway on our way to the show.</p>
<p><em><br /></em></p>
<p><em>Photos by Michael Schrader</em></p>
