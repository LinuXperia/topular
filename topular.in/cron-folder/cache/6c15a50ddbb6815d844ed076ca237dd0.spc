  ";s:7:"attribs";a:0:{}s:8:"xml_base";s:35:"http://progressillinois.com/explore";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";s:5:"child";a:3:{s:0:"";a:7:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:72:"New StreetWise-affiliated Produce Carts To Pop Up Across Chicago In June";s:7:"attribs";a:0:{}s:8:"xml_base";s:35:"http://progressillinois.com/explore";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:126:"http://feedproxy.google.com/~r/ProgressIllinois/~3/CvCBETcd9oE/new-streetwise-affiliated-produce-carts-pop-across-chicago-june";s:7:"attribs";a:0:{}s:8:"xml_base";s:35:"http://progressillinois.com/explore";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:3554:"<div class="field field-type-link field-field-news-links">
