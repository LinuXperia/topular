  ";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:5:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:59:"Indiana Department of Education Releases 2014 ISTEP+ Scores";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:82:"http://www.doe.in.gov/news/indiana-department-education-releases-2014-istep-scores";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:991:"<div class="field field-name-body field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even" property="content:encoded"><p>INDIANAPOLIS – The Indiana Department of Education released statewide and local 2014 ISTEP+ scores today.  The data show increased scores throughout subject areas at the statewide level.  Specifically, 80.7% of Indiana students passed the English/Language Arts portion of the test, up from 79.5% in 2013.  In Math, 83.5% passed, up from 83.0% in 2013.  Finally, 74.7% of Indiana students passed both English/Language Arts and Math, up from 73.7% last year.</p></div></div></div><div class="field field-name-field-release-date field-type-date field-label-above"><div class="field-label">Release Date:&nbsp;</div><div class="field-items"><div class="field-item even"><span class="date-display-single" property="dc:date" datatype="xsd:dateTime" content="2014-08-04T00:00:00-04:00">Mon, 08/04/2014</span></div></div></div>";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:31:"Mon, 04 Aug 2014 12:23:29 +0000";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"guid";a:1:{i:0;a:5:{s:4:"data";s:29:"3123 at http://www.doe.in.gov";s:7:"attribs";a:1:{s:0:"";a:1:{s:11:"isPermaLink";s:5:"false";}}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}}s:32:"http://purl.org/dc/elements/1.1/";a:1:{s:7:"creator";a:1:{i:0;a:5:{s:4:"data";s:4:"hart";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}}}}i:4;a:6:{s:4:"data";s:28:"
    
    
    
     
 
 
  ";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:5:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:89:"Indiana Department of Education to Host Teen Dating Violence Prevention Training Tomorrow";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:110:"http://www.doe.in.gov/news/indiana-department-education-host-teen-dating-violence-prevention-training-tomorrow";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:1007:"<div class="field field-name-body field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even" property="content:encoded"><p>INDIANAPOLIS – Tomorrow, the Indiana Department of Education and the Indiana School Safety Academy are co-sponsoring a regional training titled “Teen Dating Violence/Sexual Assault: Creating a Safe Environment at School.” This event is designed to help create a culture of safety in Indiana schools and effectively aid victims of violence and assault. It will be held on Friday, July 18th at Harrison College in Indianapolis from 8:30am to 3:30pm and is open to the press.</p></div></div></div><div class="field field-name-field-release-date field-type-date field-label-above"><div class="field-label">Release Date:&nbsp;</div><div class="field-items"><div class="field-item even"><span class="date-display-single" property="dc:date" datatype="xsd:dateTime" content="2014-07-17T00:00:00-04:00">Thu, 07/17/2014</span></div></div></div>";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:31:"Thu, 17 Jul 2014 19:06:2