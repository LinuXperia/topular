  ";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:5:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:61:"Indiana Department of Education Releases Graduation Rate Data";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:87:"http://www.doe.in.gov/news/indiana-department-education-releases-graduation-rate-data-0";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:1059:"<div class="field field-name-body field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even" property="content:encoded"><p>INDIANAPOLIS – The Indiana Department of Education released graduation rate data today for the 2012-2013 school year.  The data show that the state’s non-waiver graduation rate was 81.72%, up from 80.46% in 2012.  The overall graduation rate for all schools was 88.6%, virtually unchanged from the prior year’s rate of 88.7%.  Non-waiver data is important for federal reporting requirements, as well as providing a more full picture of the number of students that are successfully passing their end of course assessments.</p></div></div></div><div class="field field-name-field-release-date field-type-date field-label-above"><div class="field-label">Release Date:&nbsp;</div><div class="field-items"><div class="field-item even"><span class="date-display-single" property="dc:date" datatype="xsd:dateTime" content="2014-04-16T00:00:00-04:00">Wed, 04/16/2014</span></div></div></div>";s:7:"attribs";a:0:{}s:8:"xml_base";s:43:"http://www.doe.in.gov/taxonomy/term/150/all";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:31:"Wed, 16 Apr 2014 14:52:13 +